{
  "name": "dms-cli",
  "version": "0.0.1",
  "author": "jiaozhiye <jiaozhiye@163.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development ENV_CONFIG=dev node --max-old-space-size=4096 build/dev.js",
    "dev": "cross-env NODE_ENV=development ENV_CONFIG=dev webpack-dev-server --inline --hot --open --progress --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production ENV_CONFIG=prod node --max-old-space-size=32768 build/prod.js",
    "build-dev": "cross-env NODE_ENV=production ENV_CONFIG=dev node --max-old-space-size=32768 build/prod.js",
    "build-tst": "cross-env NODE_ENV=production ENV_CONFIG=tst node --max-old-space-size=32768 build/prod.js",
    "build-uat": "cross-env NODE_ENV=production ENV_CONFIG=uat node --max-old-space-size=32768 build/prod.js",
    "build-pre": "cross-env NODE_ENV=production ENV_CONFIG=pre node --max-old-space-size=32768 build/prod.js",
    "lint": "eslint --ext .js,.vue ./src",
    "lint:fix": "eslint --fix --ext .js,.vue ./src",
    "format": "prettier --write \"./src/**/*.{js,vue,jsx,css,scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "add-dom-event-listener": "^1.1.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "countup.js": "^2.0.4",
    "echarts": "^4.8.0",
    "element-ui": "2.13.2",
    "js-cookie": "^2.2.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "mockjs": "^1.1.0",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^4.2.0",
    "vue": "2.6.11",
    "vue-i18n": "^8.17.4",
    "vue-router": "^3.3.2",
    "vuex": "^3.4.0",
    "xlsx": "^0.15.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@vue/babel-preset-jsx": "^1.1.2",
    "autoprefixer": "^9.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.2",
    "ora": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
